{
  "name": "jio-chatbot-nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\" --names \"Backend,Frontend\" --prefix-colors \"cyan,magenta\"",
    "dev:frontend": "next dev",
    "dev:backend": "node scripts/start-backend.js",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "install:backend": "pip install -r requirements.txt",
    "setup": "npm install && npm run install:backend",
    "test:setup": "node scripts/test-setup.js"
  },
  "dependencies": {
    "axios": "^1.7.4",
    "clsx": "^2.1.1",
    "framer-motion": "^11.2.14",
    "lucide-react": "^0.365.0",
    "mongodb": "^6.18.0",
    "next": "14.2.3",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-dropzone": "^14.3.4",
    "react-hot-toast": "^2.5.1",
    "tailwind-merge": "^2.2.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@types/node": "^22.5.4",
    "@types/react": "^18.3.4",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.0.0",
    "globals": "^15.0.0",
    "postcss": "^8.4.41",
    "tailwindcss": "^3.4.10",
    "typescript": "^5.6.2"
  }
}
